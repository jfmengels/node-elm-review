{
  "name": "elm-review",
  "version": "2.12.0",
  "description": "Run elm-review from Node.js",
  "type": "commonjs",
  "engines": {
    "node": ">=14.17"
  },
  "packageManager": "npm@10.7.0",
  "scripts": {
    "test": "turbo run testing",
    "elm-format": "elm-format --validate ast-codec init-templates new-package parseElm review template",
    "eslint-check": "eslint . --report-unused-disable-directives --max-warnings=0",
    "eslint-fix": "npm run eslint-check -- --fix",
    "prettier-check": "prettier . --check",
    "prettier-fix": "prettier . --write",
    "tsc": "tsc && tsc --project tsconfig.no-implicit-any.json",
    "tsc-watch": "tsc --watch",
    "elm-tests": "(cd template/ && elm make src/Elm/Review/Main.elm --output=/dev/null && elm-test)",
    "jest": "npx jest",
    "test-run": "(cd test/ && ./run.sh)",
    "test-run-record": "(cd test/ && ./run.sh record)",
    "prepare": "elm-tooling install",
    "check-engines": "ls-engines"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jfmengels/node-elm-review.git"
  },
  "bin": {
    "elm-review": "bin/elm-review"
  },
  "files": [
    "/ast-codec/src/**/*.elm",
    "/ast-codec/elm.json",
    "/bin/",
    "/lib/**/*.js",
    "/parseElm/**/*.elm",
    "/parseElm/elm.json",
    "/new-package/elm-review-package-tests/",
    "/new-package/github/",
    "/new-package/maintenance/",
    "/init-templates/",
    "/vendor/",
    "/template/src/**/*.elm"
  ],
  "keywords": [
    "elm",
    "review",
    "elm-review",
    "lint",
    "linter",
    "check",
    "cli",
    "AST"
  ],
  "author": "Jeroen Engels",
  "funding": {
    "url": "https://github.com/sponsors/jfmengels"
  },
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/jfmengels/node-elm-review/issues"
  },
  "homepage": "https://github.com/jfmengels/node-elm-review#readme",
  "dependencies": {
    "chalk": "^4.0.0",
    "chokidar": "^3.5.2",
    "cross-spawn": "^7.0.3",
    "elm-solve-deps-wasm": "^1.0.2",
    "fastest-levenshtein": "^1.0.16",
    "find-up": "^4.1.0",
    "folder-hash": "^3.3.0",
    "fs-extra": "^9.0.0",
    "glob": "^10.2.6",
    "globby": "^13.2.2",
    "got": "^11.8.5",
    "graceful-fs": "^4.2.11",
    "minimist": "^1.2.6",
    "ora": "^5.4.0",
    "path-key": "^3.1.1",
    "prompts": "^2.2.1",
    "rimraf": "^5.0.0",
    "strip-ansi": "^6.0.0",
    "terminal-link": "^2.1.1",
    "which": "^2.0.2",
    "wrap-ansi": "^7.0.0"
  },
  "devDependencies": {
    "@types/folder-hash": "^4.0.4",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.5.2",
    "@types/minimist": "^1.2.5",
    "@types/node": "^14.18.63",
    "@types/prompts": "^2.4.9",
    "@types/which": "^2.0.2",
    "@types/wrap-ansi": "~8.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "elm-test": "^0.19.1-revision12",
    "elm-tooling": "^1.15.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-turbo": "^2.0.4",
    "eslint-config-xo": "^0.43.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-unicorn": "^52.0.0",
    "jest": "^27.3.1",
    "jest-file-snapshot": "^0.5.0",
    "ls-engines": "^0.9.2",
    "prettier": "^2.8.8",
    "turbo": "^2.0.4",
    "typescript": "~5.5.2",
    "uglify-js": "^3.18.0"
  }
}
